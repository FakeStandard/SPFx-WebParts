{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "pivot-sample-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/pivotSample/PivotSampleWebPart.js",
          "manifest": "./src/webparts/pivotSample/PivotSampleWebPart.manifest.json"
        }
      ]
    },
    "get-current-user-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/getCurrentUser/GetCurrentUserWebPart.js",
          "manifest": "./src/webparts/getCurrentUser/GetCurrentUserWebPart.manifest.json"
        }
      ]
    },
    "simple-list-operations-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/simpleListOperations/SimpleListOperationsWebPart.js",
          "manifest": "./src/webparts/simpleListOperations/SimpleListOperationsWebPart.manifest.json"
        }
      ]
    },
    "react-crud-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/reactCrud/ReactCrudWebPart.js",
          "manifest": "./src/webparts/reactCrud/ReactCrudWebPart.manifest.json"
        }
      ]
    },
    "handling-checkbox-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/handlingCheckbox/HandlingCheckboxWebPart.js",
          "manifest": "./src/webparts/handlingCheckbox/HandlingCheckboxWebPart.manifest.json"
        }
      ]
    },
    "icon-sample-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/iconSample/IconSampleWebPart.js",
          "manifest": "./src/webparts/iconSample/IconSampleWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "PivotSampleWebPartStrings": "lib/webparts/pivotSample/loc/{locale}.js",
    "GetCurrentUserWebPartStrings": "lib/webparts/getCurrentUser/loc/{locale}.js",
    "SimpleListOperationsWebPartStrings": "lib/webparts/simpleListOperations/loc/{locale}.js",
    "ReactCrudWebPartStrings": "lib/webparts/reactCrud/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "HandlingCheckboxWebPartStrings": "lib/webparts/handlingCheckbox/loc/{locale}.js",
    "IconSampleWebPartStrings": "lib/webparts/iconSample/loc/{locale}.js"
  }
}
